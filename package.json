{
  "name": "address-to-image",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build:ts && npm run tslint",
    "build:debug": "npm run build && npm run watch:debug",
    "serve": "node dist/adress-to-image/src/example/server.js",
    "serve:debug": "nodemon dist/address-to-image/src/example/server.js",
    "test": "jest --forceExit",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch:ts\" \"npm run serve\"",
    "watch:debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch:ts\" \"npm run serve:debug\"",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kshintaku/address-to-image.git"
  },
  "bugs": {
    "url": "https://github.com/kshintaku/address-to-image/issues"
  },
  "homepage": "https://github.com/kshintaku/address-to-image#readme",
  "dependencies": {
    "@google-cloud/storage": "^2.5.0",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^2.0.0",
    "express": "^4.16.4",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.7",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/express": "^4.16.1",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.16",
    "concurrently": "^3.4.0",
    "nodemon": "^1.11.0",
    "tslint": "^5.8.0",
    "typescript": "^2.4.2"
  }
}
